<view class="success flex flex-col items-center justify-between w-full">
  <view class="success-content content flex flex-col">
    <view class="success-label text-center">{{label}}</view>
    <view class="success-text text-center">{{text}}</view>
    <rich-text class="success-sub-text text-center" nodes="{{htmlNodes}}" />
  </view>
  <image class="success-img" src="/assets/images/thanks-banner.png" />
</view>