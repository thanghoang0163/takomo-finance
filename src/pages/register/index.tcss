.register {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  min-height: 100vh;
  background: linear-gradient(
    to bottom,
    rgba(114, 9, 183, 1) 15%,
    rgba(255, 255, 255, 1) 20%
  );
}

.register-container {
  width: 100%;
}

.register-title {
  margin-top: 20px;
}
